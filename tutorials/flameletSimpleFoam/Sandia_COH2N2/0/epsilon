FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inletfuel
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.0003206;
        value           uniform 3.337e+06;
    }
    inletair
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.021;
        value           uniform 0.00041;
    }
    outlet
    {
        type            zeroGradient;
    }
    axis
    {
        type            empty;
    }
    sidewall
    {
        type            zeroGradient;
    }
    burnerwall
    {
        type            epsilonWallFunction;
        value           uniform 0.00041;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


